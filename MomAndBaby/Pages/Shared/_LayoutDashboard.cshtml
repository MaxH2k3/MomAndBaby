@using MomAndBaby.Utilities.Constants

<!doctype html>
<html class="no-js" lang="en">

<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />

    <title>Demo : eCommerce - Dashboards | Sneat - Bootstrap Dashboard PRO</title>


    <meta name="description" content="Most Powerful &amp; Comprehensive Bootstrap 5 Admin Dashboard built for developers!" />
    <meta name="keywords" content="dashboard, bootstrap 5 dashboard, bootstrap 5 design, bootstrap 5">
    <!-- Canonical SEO -->
    <link rel="canonical" href="https://themeselection.com/item/sneat-dashboard-pro-bootstrap/">
    <!-- End Google Tag Manager -->
    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="https://demos.themeselection.com/sneat-bootstrap-html-admin-template/assets/img/favicon/favicon.ico" />

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com/">
    <link rel="preconnect" href="https://fonts.gstatic.com/" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&amp;display=swap" rel="stylesheet">

    <!-- Icons -->
    <link rel="stylesheet" href="~/ok/assets/vendor/fonts/boxicons.css" />
    <link rel="stylesheet" href="~/ok/assets/vendor/fonts/fontawesome.css" />
    <link rel="stylesheet" href="~/ok/assets/vendor/fonts/flag-icons.css" />

    <!-- Core CSS -->
    <link rel="stylesheet" href="~/ok/assets/vendor/css/rtl/core.css" />
    <link rel="stylesheet" href="~/ok/assets/vendor/css/rtl/theme-default.css" />
    <link rel="stylesheet" href="~/ok/assets/css/demo.css" />

    <!-- Vendors CSS -->
    <link rel="stylesheet" href="~/ok/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" />
    <link rel="stylesheet" href="~/ok/assets/vendor/libs/typeahead-js/typeahead.css" />

    @RenderSection("Styles", required: false)

    <!-- Helpers -->
    <script src="~/ok/assets/vendor/js/helpers.js"></script>
    <!--! Template customizer & Theme config files MUST be included after core stylesheets and helpers.js in the <head> section -->
    <!--? Template customizer: To hide customizer set displayCustomizer value false in config.js.  -->
    <script src="~/ok/assets/vendor/js/template-customizer.js"></script>
    <script src="~/ok/assets/js/config.js"></script>
</head>
<body class="skin-dark">
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-content-navbar  ">
        @{
            await Html.RenderPartialAsync(ConstantPage.DashboardPage.Menu);
        }

        <div class="layout-container">

            <!-- Layout container -->
            <div class="layout-page">

                <!-- Layout navbar -->
                @{
                    await Html.RenderPartialAsync(ConstantPage.DashboardPage.Header);
                }

                <div class="content-wrapper">

                    <!-- Content -->

                    <div class="container-xxl flex-grow-1 container-p-y">

                        @RenderBody()
                    </div>
                
                </div>

                
                
                @{
                    await Html.RenderPartialAsync(ConstantPage.DashboardPage.Footer);
                }

            </div>
            <!-- / Layout page -->
        </div>



        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>


        <!-- Drag Target Area To SlideIn Menu On Small Screens -->
        <div class="drag-target"></div>

    </div>
    <!-- / Layout wrapper -->

    <!-- Core JS -->
    <script src="~/ok/assets/vendor/libs/jquery/jquery.js"></script>
    <script src="~/ok/assets/vendor/libs/popper/popper.js"></script>
    <script src="~/ok/assets/vendor/js/bootstrap.js"></script>
    <script src="~/ok/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/ok/assets/vendor/libs/hammer/hammer.js"></script>
    <script src="~/ok/assets/vendor/libs/i18n/i18n.js"></script>
    <script src="~/ok/assets/vendor/libs/typeahead-js/typeahead.js"></script>
    <script src="~/ok/assets/vendor/js/menu.js"></script>

    <!-- Main JS -->
    <script src="~/ok/assets/js/main.js"></script>
    
    <!-- Custom JS -->
    <script src="~/dashboard/assets/js/custom.js"></script>

    @RenderSection("Scripts", required: false)
    
    @* active sidebar *@
        @{
            var currentMenu = 0;
            currentMenu = ViewData[VariableConstant.CurrentMenu] != null ? (int)ViewData[VariableConstant.CurrentMenu]! : currentMenu;
        }
     <script>
        $(document).ready(function () {
            let currentMenu = @(currentMenu);
            changeActiveMenu(currentMenu);
        });
     </script>
    
    </body>

</html>